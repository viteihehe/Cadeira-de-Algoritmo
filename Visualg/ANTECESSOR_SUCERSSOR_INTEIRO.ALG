Algoritmo "Antecessor_Sucerssor_inteiro"

  var

    A : Real
    B : real
    R : real

     inicio

           Escreval("Insira o valor desejado:")
           leia(R)

             A := R-1
             B := R+1
             
           escreva("Antecessor:", A, " Sucessor:" , B)
           
fimalgoritmo