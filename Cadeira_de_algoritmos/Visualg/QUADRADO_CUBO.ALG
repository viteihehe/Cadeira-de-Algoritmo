Algoritmo "Quadrado_Cubo"

  var

    A : real
    B : real
    
      inicio
      
        escreval("Insira o valor para elevar ao a quadrado")
        leia(A)
        
        escreval("Insira o valor para elevar ao cubo")
        leia(B)

        A := A^2
        B := B^3
        
        escreva("Valor ao quadrado:" , A, " Valor ao cubo:" , B)
        
fimalgoritmo